<div class="page-body">

  {{!-- --------------------------------------------------------------------- --}}
  {{#if users}}
  {{#each users}}
  <div class="card card-md">
    <div class="card-stamp card-stamp-lg">
      <div class="card-stamp-icon bg-primary">
        <!-- Download SVG icon from http://tabler-icons.io/i/ghost -->
        <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24" viewBox="0 0 24 24" stroke-width="2"
          stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
          <path stroke="none" d="M0 0h24v24H0z" fill="none" />
          <path
            d="M5 11a7 7 0 0 1 14 0v7a1.78 1.78 0 0 1 -3.1 1.4a1.65 1.65 0 0 0 -2.6 0a1.65 1.65 0 0 1 -2.6 0a1.65 1.65 0 0 0 -2.6 0a1.78 1.78 0 0 1 -3.1 -1.4v-7" />
          <path d="M10 10l.01 0" />
          <path d="M14 10l.01 0" />
          <path d="M10 14a3.5 3.5 0 0 0 4 0" />
        </svg>
      </div>
    </div>
    <div class="card-body">

      <div class="row align-items-center">
        {{!-- --------------------------user profile------------------------- --}}

        <img class="userProfile" src="{{this.profileImagePath}}"  alt="">

        <div class="col-10">
          <div class="markdown text-muted">
            Name: {{this.firstName}} {{this.lastName}}
            {{!-- <a href="https://tabler-icons.io" target="_blank" rel="noopener">tabler-icons.io</a>, --}}
          </div>
          <div class="markdown text-muted">
            Gender: {{this.gender}}
            {{!-- <a href="https://tabler-icons.io" target="_blank" rel="noopener">tabler-icons.io</a>, --}}
          </div>
          <div class="markdown text-muted">
            Email: {{this.userEmail}}
            {{!-- <a href="https://tabler-icons.io" target="_blank" rel="noopener">tabler-icons.io</a>, --}}
          </div>
          <div class="markdown text-muted">
            Posts Created: {{this.posts}}
            {{!-- <a href="https://tabler-icons.io" target="_blank" rel="noopener">tabler-icons.io</a>, --}}
          </div>
          <div class="markdown text-muted">
            Posts Saved: {{this.savedPosts}}
            {{!-- <a href="https://tabler-icons.io" target="_blank" rel="noopener">tabler-icons.io</a>, --}}
          </div>
           <div class="markdown text-muted">
            Joined On: {{this.createdOn}}
            {{!-- <a href="https://tabler-icons.io" target="_blank" rel="noopener">tabler-icons.io</a>, --}}
          </div>
          {{!-- <div class="mt-3">
            <a href="https://tabler-icons.io" class="btn btn-primary" target="_blank" rel="noopener">Download icons</a>
          </div> --}}
        </div>
      </div>
    </div>
  </div>
</div>
{{/each}}
{{/if}}
{{!-- --------------------------------------------------------------------- --}}

       {{!-- pagebody  --}}
       </div>